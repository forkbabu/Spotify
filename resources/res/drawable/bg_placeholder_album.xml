<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#444444"/>
            <size android:height="320dp" android:width="320dp"/>
        </shape>
    </item>
    <item android:left="10dp" android:top="10dp" android:right="10dp" android:bottom="10dp">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="40dp" android:thickness="80dp">
            <solid android:color="#26ffffff"/>
        </shape>
    </item>
    <item android:left="112dp" android:top="112dp" android:right="112dp" android:bottom="112dp">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="10dp" android:thickness="22dp">
            <solid android:color="#26ffffff"/>
        </shape>
    </item>
</layer-list>
