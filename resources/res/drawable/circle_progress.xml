<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="@dimen/std_8dp" android:top="@dimen/std_8dp" android:right="@dimen/std_8dp" android:bottom="@dimen/std_8dp">
        <rotate android:fromDegrees="135" android:toDegrees="135" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="line">
                <stroke android:width="@dimen/stroke_shape_thickness" android:color="@color/white"/>
            </shape>
        </rotate>
    </item>
    <item android:left="@dimen/std_8dp" android:top="@dimen/std_8dp" android:right="@dimen/std_8dp" android:bottom="@dimen/std_8dp">
        <rotate android:fromDegrees="45" android:toDegrees="45" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="line">
                <stroke android:width="@dimen/stroke_shape_thickness" android:color="@color/white"/>
            </shape>
        </rotate>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270" android:toDegrees="270">
            <shape android:shape="ring" android:useLevel="true" android:thickness="@dimen/ring_shape_thickness">
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>
</layer-list>
