<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="CardText" parent="@style/CardTextBase">
    </style>
    <style name="CardTextBase" parent="@style/res_2132083459_textview_medium_light">
        <item name="android:textColor">@color/card_text_color</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:lineSpacingMultiplier">0.92</item>
    </style>
    <style name="CardTitle" parent="@style/CardTitleBase">
    </style>
