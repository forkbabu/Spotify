    <Button android:id="@+id/submit_feedback_button" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/leave_feedback" android:layout_alignParentBottom="true" android:layout_centerHorizontal="true" android:filterTouchesWhenObscured="false" style="?attr/pasteButtonStylePrimary"/>
</RelativeLayout>
