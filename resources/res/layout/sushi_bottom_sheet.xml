<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" android:background="@color/black" android:paddingBottom="96dp" android:layout_width="match_parent" android:layout_height="match_parent">
    <com.spotify.paste.spotifyicon.SpotifyIconView android:id="@+id/close_button" android:background="?android:attr/selectableItemBackground" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="24dp" android:contentDescription="@string/generic_content_description_close" app:iconSize="24dp" app:iconType="211"/>
    <com.spotify.music.sushi.badge.SushiBadgeView android:id="@+id/sushi_badge" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_margin="24dp" android:layout_centerHorizontal="true" app:badgeSize="1"/>
    <TextView android:textAppearance="@style/TextAppearance_Encore_Mesto" android:textColor="@color/gray_70" android:id="@+id/current_level_header" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="48dp" android:layout_below="@+id/sushi_badge" android:layout_centerHorizontal="true"/>
    <TextView android:textAppearance="@style/TextAppearance_Encore_Brio" android:textColor="@color/white" android:id="@+id/current_level_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:layout_below="@+id/current_level_header" android:layout_centerHorizontal="true"/>
    <TextView android:textAppearance="@style/TextAppearance_Encore_Ballad" android:textColor="@color/gray_95" android:id="@+id/current_level_subtitle" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="16dp" android:layout_below="@+id/current_level_title" android:layout_centerHorizontal="true"/>
