    <FrameLayout android:layout_width="0dp" android:layout_height="0dp" app:layout_constraintBottom_toBottomOf="0" app:layout_constraintEnd_toEndOf="0" app:layout_constraintHeight_percent="0" app:layout_constraintStart_toStartOf="0" app:layout_constraintTop_toTopOf="@+id/connect_label_top_guideline">
        <include layout="@layout/driving_connect_label"/>
    </FrameLayout>
</merge>
